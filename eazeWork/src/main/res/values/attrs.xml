<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Chip">
        <attr name="chipText" format="string" />
        <attr name="hasIcon" format="boolean" />
        <attr name="chipIcon" format="integer" />
        <attr name="closable" format="boolean" />
        <attr name="selectable" format="boolean" />
        <attr name="backgroundColor" format="color" />
        <attr name="selectedBackgroundColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="selectedTextColor" format="color" />
        <attr name="closeColor" format="color" />
        <attr name="selectedCloseColor" format="color" />
    </declare-styleable>
    <declare-styleable name="BannerSlider">
        <attr name="indicatorSize" format="dimension" />
        <attr name="selected_slideIndicator" format="reference" />
        <attr name="unselected_slideIndicator" format="reference" />
        <attr name="defaultIndicators" format="integer">
            <enum name="circle" value="0" />
            <enum name="square" value="1" />
            <enum name="round_square" value="2"/>
            <enum name="dash" value="3"/>
        </attr>
        <attr name="animateIndicators" format="boolean"/>
        <attr name="intervalSecond" format="integer"/>
        <attr name="loopSlides" format="boolean"/>
        <attr name="hideIndicators" format="boolean"/>
    </declare-styleable>
</resources>